{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["dom", "es6"],
    "strict": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "typeRoots": ["../node_modules/@types", "../@types"]
  },
  "include": ["src/**/*"]
}
